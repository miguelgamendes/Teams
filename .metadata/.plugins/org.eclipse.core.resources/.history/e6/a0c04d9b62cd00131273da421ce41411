#include <iostream>
#include <vector>
#include "Person.h"
#include "FileOps.h"

int main(){
	std::cout << "Program started." << std::endl;
	Skill skill("DB");
	std::vector<Skill> s;
	s.push_back skill;
	Person p("Rita", s);

	std::vector<Person> people;
	loadPeople(people);

	std::cout << "OUTPUT:" << std::endl;
	for(unsigned int i = 0; i < people.size(); i++){
		std::cout << i << std::endl;
		people[i].print();
	}

	return 0;
}
